#include <LiquidCrystal_I2C.h>
#include <Wire.h>
void LCD_init(LiquidCrystal_I2C Screen)
{
  uint8_t T[8] = {0xff, 0x0, 0x1b, 0x1b, 0x1b, 0x1b, 0x1b, 0xff};;
  uint8_t E[8] = {0xff, 0x0, 0xf, 0x01, 0x01, 0xf, 0x0, 0xff};
  uint8_t A[8] = {0xff, 0x1b, 0x15, 0x0e, 0x0, 0x0e, 0x0e, 0xff};
  uint8_t M[8] = {0xff, 0x0e, 0x04, 0x0a, 0x0e, 0x0e, 0x0e, 0xff};
  uint8_t S[8] = {0xff, 0x10, 0x0f, 0x0f, 0x11, 0x1e, 0x01, 0xff};
  uint8_t W[8] = {0xff, 0x0e, 0x0e, 0x0e, 0x0a, 0x0a, 0x15, 0xff};
  uint8_t I[8] = {0xff, 0x11, 0x1b, 0x1b, 0x1b, 0x1b, 0x11, 0xff};
  uint8_t P[8] = {0xff, 0x01, 0x0e, 0x0e, 0x01, 0x0f, 0x0f, 0xff};
  
  Screen.createChar(0, T);
  Screen.createChar(1, E);
  Screen.createChar(2, A);
  Screen.createChar(3, M);
  Screen.createChar(4, S);
  Screen.createChar(5, W);
  Screen.createChar(6, I);
  Screen.createChar(7, P);
  Screen.home();

  Screen.clear();
  Screen.setCursor(5, 1);
  Screen.printByte(0);
  Screen.printByte(1);
  Screen.printByte(2);
  Screen.printByte(3);
  Screen.printByte(4);
  Screen.printByte(5);
  Screen.printByte(6);
  Screen.printByte(7);
  Screen.printByte(1);
  Screen.printByte('~');
  Screen.setCursor(3, 2);
  Screen.print("Please Tap Card");
}
